<h1>New Influx Employee</h1>

<%= form_for :employee, url: employees_path do |f| %>
<p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :position %><br>
  <%= f.text_field :position %>
</p>

<p>
  <%= f.label :"twitter" %><br>
  <%= f.text_field :twitter %>
</p>

<p>
  <%= f.label :"linkedIn" %><br>
  <%= f.text_field :linkedIn %>
</p>

<p>
  <%= f.label :Bio %><br>
  <%= f.text_area :bio %>
</p>

<p>
  <%= f.submit %>
</p>
<% end %>
